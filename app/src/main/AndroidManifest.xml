<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exemple.android.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recycler.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".listview.ListViewActivity"
            android:label="@string/listView"></activity>

        <activity
            android:name=".json.JSONActivity"
            android:label="JSON"
            android:parentActivityName=".recycler.MainActivity">


            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="plus.google.com"
                    android:scheme="https" />
                <data
                    android:host="plus.google.com"
                    android:scheme="http" />
                <data
                    android:host="github.com"
                    android:scheme="https" />
                <data
                    android:host="github.com"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhotoActivity"
            android:label="@string/photoActivity"></activity>
        <activity android:name=".contacts.AllContacts"/>
        <activity android:name=".contacts.NewContactUsing"/>

    </application>

</manifest>